    "from flask import Flask, render_template\n",
    "\n",
    "app = Flask(__name__)\n",
    "\n",
    "@app.route('/hello/<user>')\n",
    "def index(user):\n",
    "    return render_template('helloworld.html', name=user)\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    app.run()"
